{
  "name": "Selfsurfing",
  "version": "1.2",
  "manifest_version": 2,
  "description": "Selfsurfing, 2012, an auto-updating clone of my browser in real time.",
  "homepage_url": "http://jonaslund.biz/works/selfsurfing/",
  "icons": { "16": "icon_16.png",
             "48": "icon_48.png",
            "128": "icon_128.png" },
  "background": {
    "scripts": ["background.js"]
  },  
  "permissions": ["tabs", "*://*/*"],
  "browser_action": {
    "name": "Start Selfsurfing",
    "default_icon": "icon.png",
    "icons": ["icon.png", "icon_on.png"]
  }
}